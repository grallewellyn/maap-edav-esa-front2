    "oida:update:master": "cross-env-shell BRANCH=master npm run oida:update:branch",
    "oida:update:release": "cross-env-shell BRANCH=latest npm run oida:update:branch",
    "oida:update:public": "npm install --@oidajs:registry=https://registry.npmjs.org/ @oidajs/core@latest @oidajs/map-ol@latest @oidajs/map-cesium@latest @oidajs/map-cesium-ol-tile-source@latest @oidajs/state-mobx@latest @oidajs/ui-react-core@latest @oidajs/ui-react-mobx@latest @oidajs/ui-react-antd@latest @oidajs/eo-mobx@latest @oidajs/eo-mobx-react@latest @oidajs/eo-geotiff@latest @oidajs/eo-adapters-adam@latest",
    "start": "cross-env TSC_WATCHFILE=UseFsEventsWithFallbackDynamicPolling NODE_PRESERVE_SYMLINKS=1 webpack-dev-server --config webpack.dev.js --mode development --progress",
    "test": "jest --config jest.config.js"
  },
