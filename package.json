    "oida:link": "node scripts/npm-deps-link @oidajs/core @oidajs/map-ol @oidajs/map-cesium @oidajs/map-cesium-ol-tile-source @oidajs/state-mobx @oidajs/ui-react-core @oidajs/ui-react-mobx @oidajs/ui-react-antd @oidajs/eo-mobx @oidajs/eo-mobx-react @oidajs/eo-geotiff @oidajs/eo-adapters-adam",
